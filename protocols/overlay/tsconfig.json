{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
  },
  "exclude": [
    "dist",
  ],
}
